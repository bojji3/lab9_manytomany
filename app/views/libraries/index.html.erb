<h1>Libraries</h1>
<%= link_to 'New Library', new_library_path %>
<ul>
  <% @libraries.each do |library| %>
    <li>
      <%= link_to library.name, library_path(library) %> |
      <%= link_to 'Edit', edit_library_path(library) %> |
      <%= link_to 'Destroy', library, method: :delete, data: { confirm: 'Are you sure?' } %>
    </li>
  <% end %>
</ul>